/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

/*底部导航公共样式*/

.foot{

  height:120rpx;
  
  width:100%;
  
  position: fixed;
  
  bottom:0;
  
  left:0;
  
  border-top:1px solid #ddd;
  
  }
  
  .foot .tab{
  
  width:25%;
  
  float: left;
  
  height:100%;
  
  font-size:25rpx;
  
  text-align:center;
  
  padding-top:15rpx;
  
  }
  
  .foot .tab image{
  
  width:45%;
  
  height:45%;
  
  }
  
  .foot .tab .tab-sele{
  
  color: #BBDEF8;
  
  }
  
  input {
  
  display:block;
  
  height:1.4rem;
  
  text-overflow:clip;
  
  overflow:hidden;
  
  white-space:nowrap;
  
  font-family:UICTFontTextStyleBody;
  
  min-height:1.4rem;
  
  }